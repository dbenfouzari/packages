    "directory": "packages/i18n"
  },
  "bugs": {
    "url": "https://github.com/dbenfouzari/packages/issues"
  },
  "homepage": "https://github.com/dbenfouzari/packages/tree/master/packages/i18n#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@types/react": "17.0.6",
    "react": "17.0.2"
  },
  "dependencies": {
    "@dbenfouzari/i18n": "^5.1.1"
  }
}
